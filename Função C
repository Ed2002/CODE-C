#include <stdio.h>
#include <stdlib.h>

//Função

//tipo de dado   //nome      //varaiavel corespondente
int                f        (int a)
{
    int fat;

    for(fat=1; a > 1; a = a -1)
    {
        fat = fat * a;
    }
//retorna fat
return fat;
}

int main()
{
 //Variaveis
 int n;
 int u;
    //Print e scan
    printf("\nDigite um número para ser Fatorado\n");
    scanf("%d",&n);
    //Faz a função
    u = f(n);
    //Printa u
    printf("\n\n%d\n\n",u);
 return 0;
}
